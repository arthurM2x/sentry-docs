# If you have any plugins, put them here!
group :jekyll_plugins do
  gem "sprockets", "~> 3.7"
  gem 'jekyll-assets'
  gem "bootstrap", "~> 4.3.1"
  gem 'jekyll-algolia', '~> 1.6.0'
  gem 'jekyll-toc', '~> 0.14.0'
  gem 'autoprefixer-rails'
